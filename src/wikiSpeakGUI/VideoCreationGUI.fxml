         <font>
            <Font size="16.0" />
         </font></Button>
      <TableView fx:id="imageView" layoutX="31.0" layoutY="101.0" prefHeight="384.0" prefWidth="965.0" AnchorPane.leftAnchor="39.0" AnchorPane.rightAnchor="39.0">
